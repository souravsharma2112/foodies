{
  "extends": "@react-native/typescript-config",
  "include": ["**/*.ts", "**/*.tsx"],
  "exclude": ["**/node_modules", "**/Pods"],
  "compilerOptions": {
    "typeRoots": ["node_modules/@types" , "src/types"],
    "types":["jest"],
    "baseUrl": "./",
    "paths": {
      "@assets/*":["src/assets/*"],
      "@components/*":["src/components/*"],
      "@utils/*":["src/utils/*"],
    }
  }
}
